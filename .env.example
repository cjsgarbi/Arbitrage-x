# Credenciais da Binance (Modo Real)
# IMPORTANTE: Nunca commite suas credenciais reais!
BINANCE_API_KEY=sua_api_key_real_aqui
BINANCE_API_SECRET=sua_api_secret_real_aqui

# Credenciais da Binance Testnet (Modo Teste)
# Obtenha em: https://testnet.binance.vision/
BINANCE_API_KEY_TESTNET=sua_api_key_testnet_aqui
BINANCE_API_SECRET_TESTNET=sua_api_secret_testnet_aqui

# Configurações de Trading
MIN_PROFIT=0.5
MAX_TRADE_SIZE=0.01
MAX_SLIPPAGE=0.1

# Configurações de Performance
CACHE_SIZE=1000
MAX_CONNECTIONS=10
WEBSOCKET_TIMEOUT=5

# Configurações de Log
LOG_LEVEL=INFO
SAVE_TRADES_LOG=true

# Configurações de Debug
DEBUG_MODE=false
SIMULATE_TRADES=true

# Timeouts e Limites
REQUEST_TIMEOUT=30
MAX_RETRIES=3
RATE_LIMIT_PER_MINUTE=1200

# Configurações de Bot
SIMULATION_MODE=false

# Configurações do ZMQ
ZMQ_HOST="localhost"
ZMQ_PORT=5555

